mixin page-nav-panner (activeIndex)
	.page_panner
		.container.flex-col.gap-4.flex(class="md:gap-6 md:items-start")
			.flex-col.justify-start.items-center.gap-6.flex
				.text-primary.text-sm(class="md:text-lg") 	الرئيسية >> 
					span.text-neutral-700.capitalize  #{activeIndex}
			.text-center.text-neutral-900.font-medium.capitalize.leading-9(class=" text-2xl md:text-3xl lg:text-4xl") #{activeIndex}
